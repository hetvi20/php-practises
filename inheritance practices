# php-practises
\\ inheritance 

<?php

class chef{
	function makepasta()
	{
		echo "a chef can make pasta very well <br>";
	}
	function makedish()
	{
		echo "a chef can normally can made variety <br>";
	}
}

class italianchief extends chef
{
function makepizza()
{
	echo"pizza make very well";
}
function makepasta()
{
	echo" pastadish";
}	
	
}
 $chef = new chef();
 $chef-> makepasta(); 

 $italianchief= new italianchief();
 $italianchief -> makepasta();

 ?>
